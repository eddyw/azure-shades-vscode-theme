{
  "name": "azure-shades",
  "displayName": "Azure Shades",
  "description": "Azure Shades, a collection of dark themes using azure colors",
  "version": "0.0.1",
  "publisher": "eddyw",
  "author": {
    "name": "Eddy Wilson"
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Themes"
  ],
  "markdown": "github",
  "keywords": [
    "clean",
    "elegant",
    "dark",
    "bluish",
    "cyan",
    "chingu",
    "wine"
  ],
  "icon": "assets/extension-marketplace-icon.png",
  "license": "MIT",
  "homepage": "https://github.com/eddyw/azure-shades-vscode-theme",
  "repository": {
    "type": "git",
    "url": "https://github.com/eddyw/azure-shades-vscode-theme"
  },
  "bugs": {
    "url": "https://github.com/eddyw/azure-shades-vscode-theme/issues"
  },
  "contributes": {
    "themes": [
      {
        "label": "Azure Shades – Default",
        "uiTheme": "vs-dark",
        "path": "./themes/Azure Shades – Default-color-theme.json"
      },
      {
        "label": "Azure Shades – Chingu Community",
        "uiTheme": "vs-dark",
        "path": "./themes/Azure Shades – Chingu-color-theme.json"
      },
      {
        "label": "Azure Shades – Wine",
        "uiTheme": "vs-dark",
        "path": "./themes/Azure Shades – Wine-color-theme.json"
      },
      {
        "label": "Azure Shades – Rose Gold",
        "uiTheme": "vs-dark",
        "path": "./themes/Azure Shades – Rose-color-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "node ./scripts/generate.js",
    "package": "vsce package --out azure-shades-vscode.vsix"
  },
  "devDependencies": {
    "immer": "^5.3.6",
    "vsce": "^1.73.0"
  }
}
